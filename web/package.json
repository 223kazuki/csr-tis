{
  "name": "layer-csr",
  "version": "0.1.0",
  "description": "CSR is a sample application showing what can be built using Layer SDKs and libraries.",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/layerhq/csr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/layerhq/csr/issues"
  },
  "homepage": "https://csr.layer.com/",
  "engines": {
    "node": ">= 6.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.2",
    "gh-pages": "^0.12.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-intl-cra": "^0.2.8",
    "react-scripts": "0.7.0"
  },
  "dependencies": {
    "autolinker": "^1.4.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "color": "^1.0.3",
    "content-type": "^1.0.2",
    "content-type-parser": "^1.0.1",
    "dateformat": "^2.0.0",
    "filesize": "^3.5.6",
    "koa": "1.2.4",
    "koa-router": "5.4.0",
    "koa-static": "2.0.0",
    "layer-ui-web": "1.0.5",
    "layer-websdk": "3.2.0",
    "lodash": "4.17.2",
    "moment": "2.18.1",
    "notifyjs": "^2.0.3",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.3.2",
    "react-fontawesome": "^1.4.0",
    "react-intl": "^2.4.0",
    "react-redux": "^4.4.5",
    "react-redux-i18n": "^1.9.0",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "0.13.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "riek": "1.0.7",
    "styled-components": "^1.4.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
